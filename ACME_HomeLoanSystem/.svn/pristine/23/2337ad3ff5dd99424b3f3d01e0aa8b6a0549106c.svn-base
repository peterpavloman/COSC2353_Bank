<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
	  xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
	  xmlns:p="http://primefaces.org/ui">

	<h:head>
        <title>Create Home Loan Account</title>
    </h:head>

	<h:body>
		<ui:composition template="template.xhtml">
			<ui:define name="top">
				<h2 class="text-l text-center">Account Details</h2>
			</ui:define>

			<ui:define name="content">
				<h:form rendered="#{userAction.loggedIn}">
					<p:wizard>
						<ui:include src="includes/basic.xhtml"/>
						<ui:include src="includes/contact.xhtml"/>
						<ui:include src="includes/employment.xhtml"/>
						<ui:include src="includes/agreement.xhtml"/>
					</p:wizard>
					<p:commandButton actionListener="#{userAction.logout()}" value="Log Out" ajax="false"/>
				</h:form>
			</ui:define>
		</ui:composition>
    </h:body>
</html>

